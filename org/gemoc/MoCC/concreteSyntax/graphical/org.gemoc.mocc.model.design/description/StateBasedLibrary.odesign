<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.obeo.fr/dsl/viewpoint/description/1.1.0" xmlns:style="http://www.obeo.fr/dsl/viewpoint/description/style/1.1.0" xmlns:tool="http://www.obeo.fr/dsl/viewpoint/description/tool/1.1.0" name="StateBasedLibrary" version="6.5.0.201210011230">
  <ownedViewpoints name="MocModeling" label="MocModeling" modelFileExtension="*.moc">
    <ownedRepresentations xsi:type="description:DiagramDescription" name="MocLib" domainClass="StateBaseRelationLibrary">
      <metamodel href="http://org.gemoc.mocc.model/0.1#/"/>
      <metamodel href="http://fr.inria.aoste.timemodel#/"/>
      <defaultLayer name="Default">
        <edgeMappings name="Transition" sourceMapping="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='StateMachineRelationDefinition']/@subNodeMappings[name='State']" targetMapping="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='StateMachineRelationDefinition']/@subNodeMappings[name='State']" targetFinderExpression="&lt;%target%>" sourceFinderExpression="&lt;%source%>" domainClass="Transition" useDomainElement="true">
          <style sizeComputationExpression="2" routingStyle="manhattan">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription showIcon="false" labelExpression="&lt;%trigger.name%>">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="RelationDefToDeclaration" sourceMapping="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='StateMachineRelationDefinition']" targetMapping="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='RelationDeclaration']" targetFinderExpression="&lt;%declaration%>">
          <style lineStyle="dash" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="7" labelFormat="italic" showIcon="false" labelExpression="defines">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="RelationLibrary" semanticCandidatesExpression="&lt;%eAllContents()%>" domainClass="RelationLibrary">
          <subContainerMappings name="StateMachineRelationDefinition" semanticCandidatesExpression="&lt;%eAllContents()%>" domainClass="StateMachineRelationDefinition">
            <subNodeMappings name="State" semanticCandidatesExpression="&lt;%eAllContents()%>" domainClass="State">
              <style xsi:type="style:EllipseNodeDescription" borderSizeComputationExpression="1" showIcon="false" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_blue']"/>
                <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_chocolate']"/>
              </style>
              <conditionnalStyles predicateExpression="&lt;%self==self.eContainer().initialState%>">
                <style xsi:type="style:EllipseNodeDescription" showIcon="false" sizeComputationExpression="2" hideLabelByDefault="true" resizeKind="NSEW">
                  <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                </style>
              </conditionnalStyles>
            </subNodeMappings>
            <style xsi:type="style:FlatContainerStyleDescription" borderSizeComputationExpression="2" showIcon="false" backgroundStyle="Liquid">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
              <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_blue']"/>
              <labelBorderStyle href="environment:/viewpoint#//@labelBorderStyles/@labelBorderStyleDescriptions.0"/>
            </style>
          </subContainerMappings>
          <subContainerMappings name="RelationDeclaration" semanticCandidatesExpression="&lt;%eAllContents()%>" domainClass="RelationDeclaration" listContainer="true">
            <subNodeMappings name="AbstractEntity" semanticCandidatesExpression="&lt;%eAllContents()%>" domainClass="AbstractEntity">
              <style xsi:type="style:SquareDescription" borderSizeComputationExpression="2" labelFormat="italic" showIcon="false" labelExpression="&lt;%&quot; \n&quot;+name%>" labelAlignment="LEFT" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
              </style>
            </subNodeMappings>
            <style xsi:type="style:ShapeContainerStyleDescription" arcWidth="2" arcHeight="2" showIcon="false" roundedCorner="true">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
            </style>
          </subContainerMappings>
        </containerMappings>
        <toolSections name="RelationTools">
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="CreateRelationDeclaration" forceRefresh="true" containerMappings="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='RelationDeclaration']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:CreateInstance" typeName="RelationDeclaration" referenceName="relationDeclarations">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="newDeclaration"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="CreateStateBasedDefinition" forceRefresh="true" containerMappings="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='StateMachineRelationDefinition']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:CreateInstance" typeName="StateMachineRelationDefinition" referenceName="relationDefinitions">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="newDefinition"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="SetDeclaration" forceRefresh="true" edgeMappings="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@edgeMappings[name='RelationDefToDeclaration']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$source%>">
                <subModelOperations xsi:type="tool:SetValue" featureName="declaration" valueExpression="&lt;%$target%>"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="CreateAbstractEntity" nodeMappings="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='RelationDeclaration']/@subNodeMappings[name='AbstractEntity']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:CreateInstance" typeName="AbstractEntity" referenceName="parameters">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="newFormalParam"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="StateMachineTools">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="CreateState" nodeMappings="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@containerMappings[name='RelationLibrary']/@subContainerMappings[name='StateMachineRelationDefinition']/@subNodeMappings[name='State']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:CreateInstance" typeName="State" referenceName="ownedStates">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="newState"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="CreateTransition" edgeMappings="//@ownedViewpoints[name='MocModeling']/@ownedRepresentations[name='MocLib']/@defaultLayer/@edgeMappings[name='Transition']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$source.eContainer()%>">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="Transition" referenceName="ownedTransitions">
                  <subModelOperations xsi:type="tool:SetValue" featureName="source" valueExpression="&lt;%$source%>"/>
                  <subModelOperations xsi:type="tool:SetValue" featureName="target" valueExpression="&lt;%$target%>"/>
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="&lt;%source.name%>2&lt;%$target.name%>"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
  </ownedViewpoints>
</description:Group>
