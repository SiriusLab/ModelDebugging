    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Parameter" eSuperTypes="#//NamedElement #//cometatime/RTInterface">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A Parameter is an abstract concept of the meta-model that aims to capture variables, parameters. A Parameter has a value (Block) and a type (Type). The value represents&#xD;&#xA;the data stored by the variable. The type represents the type of the stored value."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="name" value="TimeLibrary"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameterType" eType="#//cometadatatype/TypeDeclaration"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="value" lowerBound="1"
          eType="#//cometamoc/OpaqueBlock" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Event" abstract="true" interface="true">
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="name" value="TimeLibrary"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="eventParameters" upperBound="-1"
          eType="#//cometatime/Parameter" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="uses" upperBound="-1"
          eType="#//cometatime/ClockAbstraction" eOpposite="#//cometatime/ClockAbstraction/isReferedbyEvents"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Service" eSuperTypes="#//NamedElement #//cometatime/RTInterface">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Service The concept of service is used in the context of Cometa to abstract functions interfacing with the &quot;runtime&quot;, but also to abstract functions for communication&#xD;&#xA;between machines of control."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="name" value="TimeLibrary"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
          eType="#//cometatime/Parameter" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Send" eSuperTypes="#//cometatime/MoCInterface #//cometatime/Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Send event, This abstraction allows describing the sending events. The sending events are the result of data sending requests emitted by an entity behavior. These&#xD;&#xA;events can be parameterized. The parameters are used to specify the receiver of the request, the communication connector, or the data to be transmitted, etc."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="name" value="TimeLibrary"/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Receive" eSuperTypes="#//cometatime/MoCInterface #//cometatime/Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Receive event, This abstraction allows describing the reception events. The reception events are the result of the reception requests issued by an entity behavior. These&#xD;&#xA;events can be parameterized. The parameters are used to specify the communication connector. Within Cometa, the events are used as &quot;trigger&quot; on the transitions of the control machines."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="name" value="TimeLibrary"/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Ack" eSuperTypes="#//cometatime/MoCInterface #//cometatime/Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Ack event, The Ack events are sent back to the behavioral entity so they can continue their execution. The Ack are needed in all running control scenarios with Cometa.&#xD;&#xA;They reify the notion of permission to run."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="name" value="TimeLibrary"/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Interface" abstract="true" interface="true"
        eSuperTypes="#//NamedElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="kind" eType="#//cometatime/InterfaceKind"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MoCInterface" abstract="true" interface="true"
        eSuperTypes="#//cometatime/Interface"/>
    <eClassifiers xsi:type="ecore:EClass" name="RTInterface" abstract="true" interface="true"
        eSuperTypes="#//cometatime/Interface">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The notion of RTInterface captures the events (MoCEvent), as well as services and parameters that are used within the control behavior to interface with the communication media (shared memory such as FIFO and LIFO)."/>
