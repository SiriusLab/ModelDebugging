      <eStructuralFeatures xsi:type="ecore:EReference" name="queues" upperBound="-1"
          eType="#//cometamoc/Queue" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MoCDomain" eSuperTypes="#//NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A MoCDomain is a concept reifying the notion of MoC modeling space. A MoC modeling space captures all of the rules, properties and semantics specific to a MoC.&#xD;&#xA;The captured rules include operational behaviors defined through the notion of Behavior."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mocBehaviors" upperBound="-1"
          eType="#//cometamoc/Behavior" containment="true" eOpposite="#//cometamoc/Behavior/mocDomain"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="domainTimeConstraints"
          upperBound="-1" eType="#//cometatime/TimeConstraint" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="MoCRole">
      <eLiterals name="Orchestrator" literal="OrchestratorBehavior"/>
      <eLiterals name="PortBehavior" value="1"/>
      <eLiterals name="ConnectorBehavior" value="2"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="QueueKind">
      <eLiterals name="FIFO"/>
      <eLiterals name="LIFO" value="1"/>
    </eClassifiers>
