      <eStructuralFeatures xsi:type="ecore:EAttribute" name="roles" upperBound="-1"
          eType="#//cometamoc/MoCRole">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The &lt; role > function is defined to associate the following roles (SchedulingBehavior, PortBehavior, ConnectorBehavior) with the different behaviors, making explicit what they can be used for."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mocDomain" lowerBound="1"
          eType="#//cometamoc/MoCDomain" transient="true" derived="true" eOpposite="#//cometamoc/MoCDomain/mocBehaviors">
        <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
          <details key="name" value="mocDomain"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="queueInstances" upperBound="-1"
          eType="#//cometamoc/QueueInstance" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="rtInterfaces" upperBound="-1"
          eType="#//cometatime/RTInterface" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="behaviorTimeConstraints"
