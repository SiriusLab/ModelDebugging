  <eSubpackages name="cometastructure" nsURI="http://org.gemoc.mocc.cometastructure/1.0"
      nsPrefix="cometastructure">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="invocationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
      <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
      <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="StructureContainer" eSuperTypes="#//NamedElement #//cometastructure/Container">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A StructureContainer represents a possible configuration of interconnected components. Indeed, it allows describing new components, connectors and the mechanisms of&#xD;&#xA;the components interconnections."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="connectorInstances" upperBound="-1"
          eType="#//cometastructure/MoCConnector" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="componentInstances" upperBound="-1"
          eType="#//cometastructure/MoCComponent" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="portInstances" upperBound="-1"
          eType="#//cometastructure/MoCPort" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MoCConnector" eSuperTypes="#//cometastructure/StructureElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="MoCConnector: represents the set of connectors that can be defined from the structure abstract syntax. The connectors allow linking the components of the topology."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
          eType="#//cometastructure/MoCPort"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
          eType="#//cometastructure/MoCPort"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="kind" lowerBound="1"
          eType="#//cometastructure/MoCConnectorKind"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MoCComponent" abstract="true" interface="true"
        eSuperTypes="#//cometastructure/StructureElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="MoCComponent: abstract structural component, that is specialized into Basic and Composite components.&#xD;&#xA;The MoCComponent are virtual parallel entities used for the hierarchical description of topologies, but also as support for the mechanisms of execution control. The MoCComponent represent atomic and composite components. These two types of components have similar properties such as the ability to define sets of ports and to carry&#xD;&#xA;behaviors used for execution control. However, when they are defined separately, atomic and composite components provide different properties."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedPortInstances" upperBound="-1"
          eType="#//cometastructure/MoCPort" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MoCPort" eSuperTypes="#//cometastructure/StructureElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="MoCPort: The ports are the interconnection points for the components. Ports are connected between them by the Cometa MoCConnector.&#xD;&#xA;The MoCConnector of Cometa are structural elements to bind the ports of the various components (Part or MoCComponent).&#xD;&#xA; Links to ports are defined by the &lt;source; target > functions that combine each connector to its specific input and output ports. &#xD;&#xA;The connectors more substantially contribute to the definition of the mechanisms of execution control because they are carriers of behaviors to synchronize components.&#xD;&#xA;The MoCPort can be enriched with MoC based behaviors. When applying a MoC behavior to a MoCPort, this behavior participates in the specification of the communication protocol."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="direction" lowerBound="1"
          eType="#//cometastructure/PortKind">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The &lt;direction> property qualifies the port with a &quot;kind&quot; (in or out) to specify the direction of the communication."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//cometastructure/MoCPort"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="portInterfaces" lowerBound="1"
          upperBound="2" eType="#//cometatime/MoCInterface"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="PortKind">
      <eLiterals name="in" literal="in"/>
      <eLiterals name="out" value="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="StructureElement" abstract="true"
        interface="true" eSuperTypes="#//NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="behaviorMap" eType="#//cometamoc/Behavior"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mocDomain" eType="#//cometamoc/MoCDomain"
          derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
          <details key="derivation" value="(MoCDomain)((StructureElement)self.eContainer()).behaviorMap().eContainer()"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
          eType="#//cometatime/Parameter" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Container" abstract="true" interface="true">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parts" upperBound="-1"
          eType="#//cometastructure/Part" containment="true" eOpposite="#//cometastructure/Part/container"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CompositeComponent" eSuperTypes="#//cometastructure/MoCComponent #//cometastructure/Container">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="CompositeComponent: A CompositeComponent objectifies the notion of hierarchy and &quot;white-box&quot;. It allows hiding his sub-topologies associated with its description&#xD;&#xA;(sub-topologies are not seen as black-boxes). A CompositeComponent has the same properties as a StructureContainer with the difference that it possibly has ports and behavior when interconnected with other components of the same hierarchical level."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="connectors" upperBound="-1"
          eType="#//cometastructure/MoCConnector" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="subComponents" upperBound="-1"
          eType="#//cometastructure/MoCComponent" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="BasicComponent" eSuperTypes="#//cometastructure/MoCComponent">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="BasicComponent: atomic components of a structure Cometa. Unlike the composite components, the BasicComponent are the entities directly interfaced with the application&#xD;&#xA;blocks."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="computation" eType="#//cometamoc/OpaqueBlock"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="StructureLibrary" eSuperTypes="#//NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="StructureLibrary: is uniquely defined to contain the library of semantic layers described at the topological level. The networks of interconnected components forms the semantic layer. The library allows describing several containers of topologies (StructureContainer, CompositeComponent) and atomic (BasiCComponent) components. In Cometa, each StructureContainer represents a semantic layer."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mocStructures" upperBound="-1"
          eType="#//cometastructure/StructureContainer" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mocInterfaces" upperBound="-1"
          eType="#//cometatime/MoCInterface" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="MoCConnectorKind">
      <eLiterals name="delegation"/>
      <eLiterals name="assembly" value="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Part" eSuperTypes="#//NamedElement #//cometastructure/Container">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Part: Part are containers insofar as they are able to contain / define a semantic layer. Part refers to existing components and represents a duplication of the latter.&#xD;&#xA;The Part participates in the description of topologies by offering the possibility to describe the concept of &quot;black-box&quot; reproducing the behavior of a composite or atomic&#xD;&#xA;execution control module. Elements of type Part have ports connecting them with the rest of the structure."/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
        <details key="constraints" value="PartHasSameNumberOfPortsThanItsComponent EachPortIsDefinedInComponent NoRedondancyInPortsNames"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
        <details key="PartHasSameNumberOfPortsThanItsComponent" value="ports->size()=componentType.ownedPortInstances->size()"/>
        <details key="EachPortIsDefinedInComponent" value="ports->forAll(self.componentType.ownedPortInstances->collect(name)->includes(name))"/>
        <details key="NoRedondancyInPortsNames" value="self.ports->forAll(p1, p2 | p1&lt;>p2 implies p1.name&lt;>p2.name)"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="componentType" lowerBound="1"
          eType="#//cometastructure/MoCComponent"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ports" upperBound="-1"
          eType="#//cometastructure/MoCPort" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="container" eType="#//cometastructure/Container"
          eOpposite="#//cometastructure/Container/parts"/>
    </eClassifiers>
  </eSubpackages>
