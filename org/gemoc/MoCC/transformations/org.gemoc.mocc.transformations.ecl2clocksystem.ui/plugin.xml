<plugin>
   <extension point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="*.ecl"
            id="org.gemoc.mocc.transformations.ecl2clocksystem.ui.popupMenus.contribution.IFile">
         <menu id="org.eclipse.acceleo.module.menu" label="%menu.label" path="additionsAcceleo">
               <groupMarker name="acceleo"/> 
         </menu>
         <action
               class="org.gemoc.mocc.transformations.ecl2clocksystem.ui.popupMenus.AcceleoGenerateEcl2clocksystemAction"
               enablesFor="1"
               id="org.gemoc.mocc.transformations.ecl2clocksystem.ui.popupMenus.acceleoGenerateEcl2clocksystemAction"
               icon="icons/default.gif"
               label="%action.label"
               menubarPath="org.eclipse.acceleo.module.menu/acceleo"/>
      </objectContribution>
   </extension>
</plugin>
