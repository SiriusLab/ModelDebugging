<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.gemoc.execution.engine.io"
            name="Gemoc">
      </category>
      <view
            category="org.gemoc.execution.engine.io"
            class="org.gemoc.execution.engine.io.views.EnginesStatusView"
            icon="icons/IconeGemocModel-16.png"
            id="org.gemoc.execution.engine.io.views.EnginesStatusView"
            name="Gemoc Engines Status">
      </view>
      <view
            category="org.gemoc.execution.engine.io"
            class="org.gemoc.execution.engine.io.views.ExecutionTraceView"
            icon="icons/IconeGemocModel-16.png"
            id="org.gemoc.execution.engine.io.views.ExecutionTraceView"
            name="Gemoc Execution Trace">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="org.gemoc.execution.engine.io.views.EnginesStatusView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
    <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         <view
               id="org.gemoc.execution.engine.io.views.EnginesStatusView"
               ratio="0.5"
               relationship="stack"
               relative="org.eclipse.debug.ui.VariableView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         <view
               id="org.gemoc.execution.engine.io.views.ExecutionTraceView"
               ratio="0.5"
               relationship="stack"
               relative="org.eclipse.debug.ui.VariableView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
