<?xml version="1.0" encoding="ASCII"?>
<org.gemoc.execution.engine.example:System
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:org.gemoc.execution.engine.example="http://org.gemoc.execution.engine.example.tfsm"
    xsi:schemaLocation="http://org.gemoc.execution.engine.example.tfsm ../../org.gemoc.execution.engine.example.tfsm.model/model/Tfsm.ecore"
    name="TrafficControl XMI-style">
  <tfsms name="Control">
    <ownedStates
        name="initControl"/>
    <ownedStates
        name="Day"/>
    <ownedStates
        name="Night"/>
    <ownedClock
        name="ControlClock"/>
    <ownedTransitions
        name="initControl_to_Day"/>
    <ownedTransitions
        name="Night_to_Day"/>
    <ownedTransitions
        name="Day_to_Day"/>
    <ownedTransitions
        name="Day_to_Night"/>
    <ownedTransitions
        name="Night_to_Night"/>
  </tfsms>
  <tfsms name="Semaphore0">
    <ownedStates
        name="initSema0"
        owningTfsm="//@tfsms.1"
        outgoingTransitions="//@tfsms.1/@ownedTransitions.0"/>
    <ownedStates
        name="Red0"
        owningTfsm="//@tfsms.1"
        outgoingTransitions="//@tfsms.1/@ownedTransitions.1"
        incomingTransitions="//@tfsms.1/@ownedTransitions.0 //@tfsms.1/@ownedTransitions.2"/>
    <ownedStates
        name="Green0"
        owningTfsm="//@tfsms.1"
        outgoingTransitions="//@tfsms.1/@ownedTransitions.2"
        incomingTransitions="//@tfsms.1/@ownedTransitions.1"/>
    <ownedClock
        name="Semaphore0Clock"/>
    <ownedTransitions
        name="initSema0_to_Red0">
      <ownedGuard
          xsi:type="org.gemoc.execution.engine.example:TemporalGuard"
          name="guardInitSema0Red0"
          onClock="//@globalClocks.0"
          afterDuration="1"/>
    </ownedTransitions>
    <ownedTransitions
        name="Red0_to_Green0">
      <ownedGuard
          xsi:type="org.gemoc.execution.engine.example:TemporalGuard"
          name="guardRed0Green0"
          onClock="//@globalClocks.0"
          afterDuration="2"/>
    </ownedTransitions>
    <ownedTransitions
        name="Green0_to_Red0">
      <ownedGuard
          xsi:type="org.gemoc.execution.engine.example:EventGuard"
          name="guardGreen0Red0"
          triggeringEvent="//@globalEvents.0"/>
    </ownedTransitions>
  </tfsms>
  <globalClocks
      name="minutes"/>
  <globalEvents
      name="switch"/>
</org.gemoc.execution.engine.example:System>
