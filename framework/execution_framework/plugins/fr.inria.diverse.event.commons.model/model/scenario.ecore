      <eGenericType eTypeParameter="#//ElementReference/T"/>
    </eStructuralFeatures>
    <eGenericSuperTypes eClassifier="#//ElementProvider">
      <eTypeArguments eTypeParameter="#//ElementReference/T"/>
    </eGenericSuperTypes>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElementQuery" abstract="true">
    <eTypeParameters name="T"/>
