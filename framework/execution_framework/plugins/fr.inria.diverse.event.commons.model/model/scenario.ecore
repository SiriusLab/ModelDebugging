      <eBounds eClassifier="ecore:EClass platform:/plugin/fr.inria.diverse.event.commons.model/model/property.ecore#//StateProperty">
        <eTypeArguments/>
      </eBounds>
    </eTypeParameters>
    <eStructuralFeatures xsi:type="ecore:EReference" name="property" containment="true">
      <eGenericType eTypeParameter="#//Stage/P"/>
    </eStructuralFeatures>
    <eGenericSuperTypes eClassifier="#//Phase">
      <eTypeArguments eTypeParameter="#//Stage/E"/>
    </eGenericSuperTypes>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElementProvider" abstract="true">
    <eTypeParameters name="T"/>
  </eClassifiers>
