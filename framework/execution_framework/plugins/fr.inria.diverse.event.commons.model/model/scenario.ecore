      <eGenericType eTypeParameter="#//Scenario/P"/>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Event" abstract="true">
    <eTypeParameters name="T"/>
