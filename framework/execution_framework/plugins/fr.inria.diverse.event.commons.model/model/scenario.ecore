      <eBounds eClassifier="ecore:EClass platform:/plugin/fr.inria.diverse.event.commons.model/model/property.ecore#//StateProperty">
        <eTypeArguments eTypeParameter="#//ElementQuery/T"/>
      </eBounds>
    </eTypeParameters>
    <eStructuralFeatures xsi:type="ecore:EReference" name="query" containment="true">
      <eGenericType eTypeParameter="#//ElementQuery/P"/>
    </eStructuralFeatures>
    <eGenericSuperTypes eClassifier="#//ElementProvider">
      <eTypeArguments eTypeParameter="#//ElementQuery/T"/>
    </eGenericSuperTypes>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TestSuite" abstract="true">
    <eTypeParameters name="T">
      <eBounds eClassifier="#//Scenario">
