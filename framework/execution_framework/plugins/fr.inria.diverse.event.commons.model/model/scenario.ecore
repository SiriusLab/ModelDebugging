      <eGenericType eTypeParameter="#//TestSuite/T"/>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Date" abstract="true">
    <eTypeParameters name="E">
      <eBounds eClassifier="#//Event">
        <eTypeArguments/>
      </eBounds>
    </eTypeParameters>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eGenericSuperTypes eClassifier="#//Phase">
      <eTypeArguments eTypeParameter="#//Date/E"/>
    </eGenericSuperTypes>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Phase" abstract="true">
    <eTypeParameters name="E">
      <eBounds eClassifier="#//Event">
