      <eGenericType eClassifier="#//ElementProvider">
        <eTypeArguments eTypeParameter="#//Event/T"/>
      </eGenericType>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Stage" abstract="true">
