      <eGenericType eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EMap">
        <eTypeArguments eClassifier="#//ProcessVariable"/>
        <eTypeArguments eClassifier="#//ContextVariable"/>
      </eGenericType>
    </eStructuralFeatures>
