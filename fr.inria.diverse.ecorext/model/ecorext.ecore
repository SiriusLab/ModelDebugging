<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ecorext" nsURI="http://ecorext/1.0" nsPrefix="ecorext">
  <eClassifiers xsi:type="ecore:EClass" name="Ecorext">
    <eStructuralFeatures xsi:type="ecore:EReference" name="newPackages" upperBound="-1"
        eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EPackage" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="classesExtensions" upperBound="-1"
        eType="#//ClassExtension" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ClassExtension">
    <eStructuralFeatures xsi:type="ecore:EReference" name="extendedExistingClass"
        eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EClass"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="newProperties" upperBound="-1"
        eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EStructuralFeature"
        containment="true"/>
  </eClassifiers>
</ecore:EPackage>
