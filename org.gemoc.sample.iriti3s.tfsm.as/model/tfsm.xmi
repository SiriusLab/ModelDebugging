<?xml version="1.0" encoding="ASCII"?>
<org.gemoc.sample.iriti3s.tfsm:System
	xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:org.gemoc.sample.iriti3s.tfsm="http://org.gemoc.sample.iriti3s.tfsm"
	xsi:schemaLocation="http://org.gemoc.sample.iriti3s.tfsm ../metamodel/tfsm.ecore">
	<ownedTfsms name="Interrupteur" initialState="//@ownedTfsms.0/@ownedStates.0">
		<ownedStates name="Off"
			incomingTransitions="//@ownedTfsms.0/@ownedStates.1/@outgoingTransitions.0">
			<outgoingTransitions name="SwitchOn"
				target="//@ownedTfsms.0/@ownedStates.1" generatedEvents="//@ownedTfsms.0/@ownedEvents.1">
				<ownedGuard xsi:type="org.gemoc.sample.iriti3s.tfsm:EventGuard"
					name="SwitchOnOnUserInteraction" triggeringEvent="//@ownedTfsms.0/@ownedEvents.0" />
			</outgoingTransitions>
		</ownedStates>
		<ownedStates name="On"
			incomingTransitions="//@ownedTfsms.0/@ownedStates.0/@outgoingTransitions.0">
			<outgoingTransitions name="SwitchOff"
				target="//@ownedTfsms.0/@ownedStates.0" generatedEvents="//@ownedTfsms.0/@ownedEvents.2">
				<ownedGuard xsi:type="org.gemoc.sample.iriti3s.tfsm:TemporalGuard"
					name="WaitForSomeTime" onClock="//@ownedTfsms.0/@ownedClock"
					afterDuration="10" />
			</outgoingTransitions>
		</ownedStates>
		<ownedEvents name="UserInteraction" />
		<ownedEvents name="LogSwitchOn"
			generatedByTransitions="//@ownedTfsms.0/@ownedStates.0/@outgoingTransitions.0" />
		<ownedEvents name="LogSwitchOff"
			generatedByTransitions="//@ownedTfsms.0/@ownedStates.1/@outgoingTransitions.0" />
		<ownedClock name="MyClock" />
	</ownedTfsms>
</org.gemoc.sample.iriti3s.tfsm:System>
