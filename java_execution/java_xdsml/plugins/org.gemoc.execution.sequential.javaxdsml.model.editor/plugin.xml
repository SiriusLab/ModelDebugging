<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

  
   <extension
          point="org.eclipse.ui.perspectiveExtensions">
       <perspectiveExtension
             targetID="org.eclipse.sirius.ui.tools.perspective.design">
          <newWizardShortcut
                id="org.gemoc.executionengine.java.sequential_xdsml.presentation.Sequential_xdsmlModelWizardID">
          </newWizardShortcut>
       </perspectiveExtension>
    </extension>



   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated sequential_xdsml -->
      <category
            id="org.gemoc.gemoc_language_workbench.ui"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.gemoc.executionengine.java.sequential_xdsml.presentation.Sequential_xdsmlModelWizardID"
            name="%_UI_Sequential_xdsmlModelWizard_label"
            class="org.gemoc.execution.sequential.javaxdsml.presentation.Sequential_xdsmlModelWizard"
            category="org.gemoc.gemoc_language_workbench.ui"
            icon="icons/full/obj16/Sequential_xdsmlModelFile.gif">
         <description>%_UI_Sequential_xdsmlModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated sequential_xdsml -->
      <editor
            id="org.gemoc.executionengine.java.sequential_xdsml.presentation.Sequential_xdsmlEditorID"
            name="%_UI_Sequential_xdsmlEditor_label"
            icon="icons/full/obj16/Sequential_xdsmlModelFile.gif"
            extensions="xdsml"
            class="org.gemoc.execution.sequential.javaxdsml.presentation.Sequential_xdsmlEditor"
            contributorClass="org.gemoc.execution.sequential.javaxdsml.presentation.Sequential_xdsmlActionBarContributor">
      </editor>
   </extension>
</plugin>
